	private CommonTest commonTest;
	
	@PostConstruct
	public void initRest(){
		sucesses = "Sucessfully Loaded..!!!";
		//commonTest = new CommonTest();
		//commonTest.setLname("Weerarathne");
	}

	public String getLname() {
		return lname;
	}

